{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-undef": 0,
        "padded-blocks": 0,
        "no-prototype-builtins": 0,
        "consistent-return": 0,
        "space-before-blocks": 0,
        "keyword-spacing": 0,
        "quotes": 0,
        "no-unused-vars": 0,
        "eol-last": 0
    }
}